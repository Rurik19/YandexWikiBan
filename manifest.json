{

  "description": "Removes Wikipedia links from Yandex search results",
  "manifest_version": 2,
  "name": "YandexWikiBan",
  "version": "1.0",
  "homepage_url": "https://github.com/Rurik19/YandexWikiBan",
  "icons": {
    "48": "icons/border-48.png"
  },

  "content_scripts": [
    {
      "matches": ["*://*.yandex.ru/*"],
      "js": ["wikiban.js"]
    }
  ]

}
